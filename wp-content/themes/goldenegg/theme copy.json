{
"version": 1,
"settings": {
	"layout": {
		"contentSize": "800px",
		"wideSize": "1200px"
	},
	"border": {
		"customColor": false,
		"customRadius": false,
		"customStyle": false,
		"customWidth": false
	},
	"color": {
		"link": false,
		"custom": false,
		"customGradient": false,
		"gradients": [],
		"customDuotone": false,
		"duotone": [],
		"palette": [
			{
			  "slug": "foreground",
			  "color": "#111",
			  "name": "Foreground"
			},
			{
			  "slug": "background",
			  "color": "#f9f9f9",
			  "name": "Background"
			},
			{
			  "slug": "primary",
			  "color": "#547e2a",
			  "name": "Primary"
			},
			{
			  "slug": "secondary",
			  "color": "#007cba",
			  "name": "Secondary"
			},
			{
			  "slug": "tertiary",
			  "color": "#e8c200",
			  "name": "Tertiary"
			},
			{
			  "slug": "gray",
			  "color": "#333",
			  "name": "Gray"
			},
			{
			  "slug": "gray-light",
			  "color": "#eee",
			  "name": "Gray Light"
			},
			{
			  "slug": "red",
			  "color": "#d3033d",
			  "name": "Red"
			}
		]
	},
	"custom": {
		"spacing": {
			"baseline": "16px"
		},
		"typography": {
			"font-primary": "var( --wp--preset--font-family--inter )",
			"line-height": {
				"gigantic": 1.1,
				"huge": 1.25,
				"large": 1.5,
				"normal": 1.7
			},
			"font-weight": {
				"normal": 400,
				"bold": 700,
				"black": 900
			},
			"letter-spacing": {
				"gigantic": "-0.04em",
				"heading": "-0.03em",
				"body": "0em",
				"spread": "0.16em"
			}
		}
	},
	"spacing": {
		"blockGap": "1.5rem",
		"customMargin": false,
		"customPadding": false,
		"units": [ "px", "%" ]
	},
	"typography": {
		"customFontSize": false,
		"customLineHeight": false,
		"dropCap": false,
		"customFontWeight": true,
		"fontFamilies": [
			{
				"fontFamily": "-apple-system,BlinkMacSystemFont,Cantarell,sans-serif",
				"name": "System Font",
				"slug": "system-font"
			},
			{
				"fontFamily": "\"Inter\", sans-serif",
				"name": "Inter",
				"slug": "inter"
			},
			{
				"fontFamily": "\"Source Serif Pro\", serif",
				"name": "Source Serif Pro",
				"slug": "source-serif-pro"
			}
		],
		"fontSizes": [
			{
				"name": "Tiny",
				"size": ".875rem",
				"slug": "tiny"
			},
			{
				"name": "Small",
				"size": "1rem",
				"slug": "small"
			},
			{
				"name": "Normal",
				"size": "1.125rem",
				"slug": "normal"
			},
			{
				"name": "Medium",
				"size": "1.25rem",
				"slug": "medium"
			},
			{
				"name": "Large",
				"size": "clamp(1.25rem, 3vw, 1.5rem)",
				"slug": "large"
			},
			{
				"name": "Huge",
				"size": "clamp(1.75rem, 4vw, 2.25rem)",
				"slug": "huge"
			},
			{
				"name": "Gigantic",
				"size": "clamp(3rem, 5vw, 3.75rem)",
				"slug": "gigantic"
			}
		]
	},
	"blocks": {
		"core/button": {
			"border": {
				"customRadius": false
			},
			"color": {
				"palette": []
			}
		},
		"core/list": {
			"color": {
				"palette": []
			}
		},
		"core/group": {
			"layout": {},
			"color": {
				"palette": [
					{
						"slug": "white",
						"color": "#fff",
						"name": "White"
					},
					{
						"slug": "gray-lighter",
						"color": "#f9f9f9",
						"name": "Gray Lighter"
					},
					{
						"slug": "green",
						"color": "#547e2a",
						"name": "Green"
					}
				]
			}
		},
		"core/separator": {
			"spacing": {
				"customMargin": false
			}
		}
	}
},
"styles": {
	"blocks": {
		"core/paragraph": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--normal)"
			}
		},
		"core/button": {
			"border": {
				"color": "currentColor",
				"radius": "0px"
			},
			"color": {
				"background": "var(--wp--preset--color--primary)",
				"text": "var(--wp--preset--color--white)"
			},
			"typography": {
				"fontSize": "var(--wp--preset--font-size--tiny)",
				"fontWeight": "var(--wp--custom--typography--font-weight--bold)"
			}
		},
		"core/post-title": {
			"spacing": {
				"margin": {
					"top": "0px",
					"bottom": "0px"
				}
			},
			"typography": {
				"fontFamily": "var(--wp--preset--font-family--system-font)",
				"letterSpacing": "var(--wp--custom--typography--letter-spacing--normal)",
				"fontWeight": "400",
				"fontSize": "var(--wp--preset--font-size--huge)"
			},
			"color": {
				"background": "var(--wp--preset--color--foreground)",
				"text": "var(--wp--preset--color--background)"
			}
		}
	},
	"elements": {
		"h1": {
			"typography": {
				"fontWeight": "700",
				"fontSize": "var(--wp--preset--font-size--gigantic)",
				"lineHeight": "var(--wp--custom--typography--line-height--gigantic)",
				"letterSpacing": "var(--wp--custom--typography--letter-spacing--gigantic)"
			}
		},
		"h2": {
			"typography": {
				"fontWeight": "900",
				"fontSize": "var(--wp--preset--font-size--huge)",
				"lineHeight": "var(--wp--custom--typography--line-height--huge)",
				"letterSpacing": "var(--wp--custom--typography--letter-spacing--huge)"
			}
		},
		"h3": {
			"typography": {
				"fontWeight": "600",
				"fontSize": "var(--wp--preset--font-size--large)",
				"lineHeight": "var(--wp--custom--typography--line-height--large)"
			}
		},
		"h4": {
			"typography": {
				"fontWeight": "900",
				"lineHeight": "var(--wp--custom--typography--line-height--huge)",
				"fontSize": "var(--wp--preset--font-size--normal)"
			}
		},
		"h5": {
			"typography": {
				"fontWeight": "600",
				"textTransform": "uppercase",
				"letterSpacing": "var(--wp--custom--typography--letter-spacing--spread)",
				"lineHeight": "var(--wp--custom--typography--line-height--huge)",
				"fontSize": "var(--wp--preset--font-size--small)"
			}
		},
		"h6": {
			"typography": {
				"fontFamily": "var(--wp--preset--font-family--system-font)",
				"fontWeight": "600",
				"textTransform": "uppercase",
				"letterSpacing": "var(--wp--custom--typography--letter-spacing--spread)",
				"lineHeight": "var(--wp--custom--typography--line-height--huge)",
				"fontSize": "var(--wp--preset--font-size--tiny)"
			}
		},
		"link": {
			"color": {
				"text": "var(--wp--preset--color--foreground)"
			}
		}
	},
	"color": {
		"background": "var(--wp--preset--color--background)",
		"text": "var(--wp--preset--color--foreground)"
	},
	"typography": {
		"fontSize": "var(--wp--preset--font-size--normal)",
		"fontFamily": "var(--wp--preset--font-family--inter)",
		"lineHeight": "1.7"
	}
}
}
